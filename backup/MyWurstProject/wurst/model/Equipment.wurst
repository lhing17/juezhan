public HashMap<int, EquipmentAttr> equipmentAttrMap = new HashMap<int, EquipmentAttr>()

public class Equipment
    item itm
    EquipmentAttr equipmentAttr
    int bonusType = 0 // 额外属性类型 0-无额外属性 1-攻击型 2-防御型 3-饰品型 4-套装型
    int holeNumber = 0 // 剩余孔数
    int hpBonus = 0
    int defenseBonus = 0
    int strengthBonus = 0
    int agilityBonus = 0
    int intelligenceBonus = 0
    int perceptionBonus = 0
    int constitutionBonus = 0
    int healingSkillBonus = 0
    int courageBonus = 0
    int luckBonus = 0
    int channelBonus = 0
    int allAttrBonus = 0


    construct(item itm, int bonusType)
        this.itm = itm
        this.bonusType = bonusType
        this.equipmentAttr = equipmentAttrMap.get(itm.getTypeId())
        EventListener.add(EVENT_PLAYER_UNIT_PICKUP_ITEM, () -> addEquipmentAttr())
        EventListener.add(EVENT_PLAYER_UNIT_DROP_ITEM, () -> removeEquipmentAttr())
    
    function addEquipmentAttr()
        if GetManipulatedItem() == this.itm and GetTriggerUnit().isHero()
            let hero = GetTriggerUnit().getHero()
            hero.u.setGeneralBonus(BONUS_TYPE_ATTACK, MODE_ADD, equipmentAttr.attack)
            hero.u.setGeneralBonus(BONUS_TYPE_ARMOR, MODE_ADD, equipmentAttr.defense + defenseBonus)
            hero.u.addStr(equipmentAttr.strength + strengthBonus)
            hero.u.addAgi(equipmentAttr.agility + agilityBonus)
            hero.u.addInt(equipmentAttr.intelligence + intelligenceBonus)

            hero.perception += equipmentAttr.perception + perceptionBonus + allAttrBonus
            hero.constitution += equipmentAttr.constitution + constitutionBonus + allAttrBonus
            hero.healingSkill += equipmentAttr.healingSkill + healingSkillBonus + allAttrBonus
            hero.courage += equipmentAttr.courage + courageBonus + allAttrBonus
            hero.luck += equipmentAttr.luck + luckBonus + allAttrBonus
            hero.channel += equipmentAttr.channel + channelBonus + allAttrBonus
            hero.uniquePerception += equipmentAttr.uniquePerception

            hero.u.setGeneralBonus(BONUS_TYPE_ATTACK_SPEED, MODE_ADD, equipmentAttr.attackSpeed.toInt())
            hero.criticalRate += equipmentAttr.criticalRate
            hero.criticalTimes += equipmentAttr.criticalTimes
            hero.dodge += equipmentAttr.dodge
            hero.vampire += equipmentAttr.vampire
            hero.lifeRegeneration += equipmentAttr.lifeRegeneration
            hero.manaRegeneration += equipmentAttr.manaRegeneration
            hero.killRegeneration += equipmentAttr.killRegeneration
            hero.attackedRegeneration += equipmentAttr.attackedRegeneration
            hero.u.setGeneralBonus(BONUS_TYPE_MAX_LIFE, MODE_ADD, equipmentAttr.maxHp.toInt() + hpBonus)
            hero.u.setGeneralBonus(BONUS_TYPE_MAX_MANA, MODE_ADD, equipmentAttr.maxMana.toInt())
            hero.u.setGeneralBonus(BONUS_TYPE_MOVE_SPEED, MODE_ADD, equipmentAttr.moveSpeed.toInt())
            


    function removeEquipmentAttr()
        if GetManipulatedItem() == this.itm and GetTriggerUnit().isHero()
            let hero = GetTriggerUnit().getHero()
            hero.u.setGeneralBonus(BONUS_TYPE_ATTACK, MODE_SUBSTRACT, equipmentAttr.attack)
            hero.u.setGeneralBonus(BONUS_TYPE_ARMOR, MODE_SUBSTRACT, equipmentAttr.defense + defenseBonus)
            hero.u.addStr(-equipmentAttr.strength + strengthBonus)
            hero.u.addAgi(-equipmentAttr.agility + agilityBonus)
            hero.u.addInt(-equipmentAttr.intelligence + intelligenceBonus)

            hero.perception -= equipmentAttr.perception + perceptionBonus + allAttrBonus
            hero.constitution -= equipmentAttr.constitution + constitutionBonus + allAttrBonus
            hero.healingSkill -= equipmentAttr.healingSkill + healingSkillBonus + allAttrBonus
            hero.courage -= equipmentAttr.courage + courageBonus + allAttrBonus
            hero.luck -= equipmentAttr.luck + luckBonus + allAttrBonus
            hero.channel -= equipmentAttr.channel + channelBonus + allAttrBonus
            hero.uniquePerception -= equipmentAttr.uniquePerception

            hero.u.setGeneralBonus(BONUS_TYPE_ATTACK_SPEED, MODE_SUBSTRACT, equipmentAttr.attackSpeed.toInt())
            hero.criticalRate -= equipmentAttr.criticalRate
            hero.criticalTimes -= equipmentAttr.criticalTimes
            hero.dodge -= equipmentAttr.dodge
            hero.vampire -= equipmentAttr.vampire
            hero.lifeRegeneration -= equipmentAttr.lifeRegeneration
            hero.manaRegeneration -= equipmentAttr.manaRegeneration
            hero.killRegeneration -= equipmentAttr.killRegeneration
            hero.attackedRegeneration -= equipmentAttr.attackedRegeneration
            hero.u.setGeneralBonus(BONUS_TYPE_MAX_LIFE, MODE_SUBSTRACT, equipmentAttr.maxHp.toInt() + hpBonus)
            hero.u.setGeneralBonus(BONUS_TYPE_MAX_MANA, MODE_SUBSTRACT, equipmentAttr.maxMana.toInt())
            hero.u.setGeneralBonus(BONUS_TYPE_MOVE_SPEED, MODE_SUBSTRACT, equipmentAttr.moveSpeed.toInt())

public class EquipmentAttr

    int itemId

    int attack
    int defense
    int strength
    int agility
    int intelligence

    int perception = 0 // 悟性
    int constitution = 0 // 根骨
    int healingSkill = 0 // 医术
    int courage = 0 // 胆魄
    int luck = 0 // 福缘
    int channel = 0 // 经脉
    int uniquePerception // 绝学领悟点

    real attackSpeed
    real criticalRate = 0 // 暴击率 0-1之间的小数
    real criticalTimes = 1 // 暴击倍数
    real dodge // 闪避
    real vampire // 技能吸血
    real lifeRegeneration // 生命回复
    real manaRegeneration // 法力回复
    real killRegeneration // 杀怪回复
    real attackedRegeneration // 受伤害回复
    real maxHp // 气血
    real maxMana // 法力上限
    real moveSpeed // 移动速度

    construct(int itemId, HashMap<string, real> attrMap)
        this.itemId = itemId
        this.attack = attrMap.get("attack").toInt()
        this.defense = attrMap.get("defense").toInt()
        this.strength = attrMap.get("strength").toInt()
        this.agility = attrMap.get("agility").toInt()
        this.intelligence = attrMap.get("intelligence").toInt()

        this.perception = (attrMap.get("perception") + attrMap.get("allAttrs")).toInt()
        this.constitution = (attrMap.get("constitution") + attrMap.get("allAttrs")).toInt()
        this.healingSkill = (attrMap.get("healingSkill") + attrMap.get("allAttrs")).toInt()
        this.courage = (attrMap.get("courage") + attrMap.get("allAttrs")).toInt()
        this.luck = (attrMap.get("luck") + attrMap.get("allAttrs")).toInt()
        this.channel = (attrMap.get("channel") + attrMap.get("allAttrs")).toInt()
        this.uniquePerception = attrMap.get("uniquePerception").toInt()
        
        this.attackSpeed = attrMap.get("attackSpeed")
        this.criticalRate = attrMap.get("criticalRate")
        this.criticalTimes = attrMap.get("criticalTimes")
        this.dodge = attrMap.get("dodge")
        this.vampire = attrMap.get("vampire")
        this.lifeRegeneration = attrMap.get("lifeRegeneration")
        this.manaRegeneration = attrMap.get("manaRegeneration")
        this.killRegeneration = attrMap.get("killRegeneration")
        this.attackedRegeneration = attrMap.get("attackedRegeneration")
        this.maxHp = attrMap.get("maxHp")
        this.maxMana = attrMap.get("maxMana")
        this.moveSpeed = attrMap.get("moveSpeed")
        equipmentAttrMap.put(itemId, this)

function HashMap<string, real>.multiPut(string attr1, real value1, string attr2, real value2, string attr3, real value3, string attr4, real value4)        
    this.put(attr1, value1)
    this.put(attr2, value2)
    this.put(attr3, value3)
    this.put(attr4, value4)
function HashMap<string, real>.multiPut(string attr1, real value1, string attr2, real value2, string attr3, real value3)        
    this.put(attr1, value1)
    this.put(attr2, value2)
    this.put(attr3, value3)
function HashMap<string, real>.multiPut(string attr1, real value1, string attr2, real value2)        
    this.put(attr1, value1)
    this.put(attr2, value2)



