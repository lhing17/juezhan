	    	        if ( I7[GetPlayerId(GetOwningPlayer(GetTriggerUnit())) * 20 + j] ) != 'AEfk' then
			            //call DisplayTextToForce(bj_FORCE_ALL_PLAYERS,"j="+I2S(j))
			            if j == wugongshu[i] then
