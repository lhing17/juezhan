	if ( ( ( EVENT_UNIT_DAMAGED == GetTriggerEventId() ) ) ) then
		call AdjustPlayerStateBJ(5 * GetUnitLevel(uc), GetOwningPlayer(u), PLAYER_STATE_RESOURCE_GOLD)
