    	set angle=Atan2(hisY - myY, hisX - myX)
    	set dist=SquareRoot(( hisX - myX ) * ( hisX - myX ) + ( hisY - myY ) * ( hisY - myY ))
        set loc2=Location(myX + perc * i * dist * Cos(angle), myY + perc * i * dist * Sin(angle))
