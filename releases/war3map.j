			set yin=yin + 0
			set yang=yang + 4
		elseif ( ( GetItemTypeId(UnitItemInSlotBJ(P4[i], bj_forLoopAIndex)) == YaoCao[10] ) ) then
			set yin=yin + 3
			set yang=yang + 4
