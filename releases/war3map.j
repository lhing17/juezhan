	if ( GetUnitState(u, UNIT_STATE_LIFE) >= 0.99 * GetUnitState(u, UNIT_STATE_MAX_LIFE) ) or ( GetRandomReal(1, 100) <= 40 ) or ( GetRandomReal(1, 100) <= 40 and GetUnitAbilityLevel(u, 'A0DN') != 0 ) then
