 local integer cx=LoadInteger(YDHT, id, - $3021938A)
 local integer i=1 + GetPlayerId(GetOwningPlayer(GetTriggerUnit()))
 local button b1=null
 local button b2=null
 local trigger t=CreateTrigger()
 local integer j=0
	set cx=cx + 3
