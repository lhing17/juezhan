function qinggongxiaoshi takes nothing returns nothing
 local timer tm=GetExpiredTimer()
 local unit u= LoadUnitHandle(YDHT, GetHandleId(tm), 0)
	call DestroyEffect(udg_JTX[GetPlayerId(GetOwningPlayer(u)) + 1])
