    call SaveReal(YDHT, StringHash(("real" )), StringHash(( I2S((GetHandleId((t)))) )), (( 0)*1.0)) // INLINED!!
    call TimerStart(t, 0.02, TRUE, function arc_Actions1)
    set t=null
endfunction
