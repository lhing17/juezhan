        		    exitwhen L7[i] > wugongshu[i]
        		    if ( ( I7[( ( ( i - 1 ) * 20 ) + L7[i] )] != 'AEfk' ) ) then
        		    else
        		        set I7[( ( ( i - 1 ) * 20 ) + L7[i] )]=P8[id]
        		        exitwhen true
        		    endif
        		    set L7[i]=L7[i] + 1
        		endloop
        	endif
        	call IncUnitAbilityLevel(u, P8[id])
        else
        	call UnitAddAbility(u, P8[id])
        	call UnitMakeAbilityPermanent(u, true, P8[id])
        	set L7[i]=1
        	loop
        	    exitwhen L7[i] > wugongshu[i]
        	    if ( ( I7[( ( ( i - 1 ) * 20 ) + L7[i] )] != 'AEfk' ) ) then
        	    else
