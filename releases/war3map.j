function cw takes integer cu returns nothing
	set u4[cu]=s4
	set s4=cu
endfunction
function Dw takes real pw,trigger Ew returns integer
 local integer cu=Cw()
 local integer h=r4
 local integer t=R2I(100. * pw) + p4
 local integer p
	set x4[cu]=Ew
	set w4[cu]=t
	loop
		set p=v4[h]
		if p < 0 or w4[p] >= t then
			set v4[h]=cu
			set v4[cu]=p
			return cu
		endif
		set h=p
	endloop
	return cu
endfunction
