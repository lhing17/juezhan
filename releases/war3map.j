		set j=1
		loop
			exitwhen j > MAX_BAN_LV_NUM
			if ( ( GetItemTypeId(GetManipulatedItem()) == S8[j] ) ) then
				call PlaySoundOnUnitBJ(Fh, 100, u)
				call CreateNUnitsAtLoc(1, R8[j], p, loc, bj_UNIT_FACING)
