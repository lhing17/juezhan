		            call IncUnitAbilityLevel(udg_boss[udg_boshu / 4], 'A0CP')
		        else
	                call UnitAddAbility(udg_boss[udg_boshu / 4], 'A0C1')
                endif
            endif
	    else
	        call PauseTimer(t)
	        call DestroyTimer(t)
        endif
    else
        call PauseTimer(t)
        call DestroyTimer(t)
    endif
	set t=null
endfunction
